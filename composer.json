{
    "name": "pepipost/pepipost-laravel-driver",
    "description": "This library adds a 'pepipost' mail driver to Laravel.",
    "keywords": ["laravel", "pepipost", "laravel mail driver"],
    "license": "MIT",
    "require": {
        "illuminate/mail": "^8.0",
        "guzzlehttp/guzzle": "^6.3 || ^7.0"
    },
    "require-dev": {
        "illuminate/container": "^8.75",
        "illuminate/filesystem": "^8.75"
    },
    "autoload": {
        "psr-4": {
            "Pepipost\\PepipostLaravelDriver\\": "src"
        }
    },
    "authors": [
        {
            "name": "Hitesh",
            "email": "hiteshpandey00@gmail.com"
        },
        {
            "name": "Rishabh",
            "email": "rishabhmishra625@gmail.com"
        },
        {
            "name": "NetcoreDevrel",
            "email": "devrel@netcorecloud.com"
        }
    ],
    "support": {
        "issues": "https://github.com/pepipost/pepipost-laravel-driver/issues"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pepipost\\PepipostLaravelDriver\\PepipostTransportServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}